{
  "mcpServers": {
    "example-server": {
      "command": "node",
      "args": ["/path/to/mcp-server.js"],
      "env": {
        "API_KEY": "your-api-key-here"
      },
      "disabled": true,
      "autoApprove": [],
      "agents": ["planning"]
    },
    "example-restricted-server": {
      "command": "python",
      "args": ["/path/to/restricted-server.py"],
      "env": {
        "API_KEY": "your-api-key-here"
      },
      "disabled": true,
      "autoApprove": [],
      "agents": ["manus"]
    }
  }
}
